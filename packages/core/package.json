{
  "name": "@livepeer/core",
  "description": "Livepeer's core vanilla JS library.",
  "license": "MIT",
  "version": "3.0.0-next.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/livepeer/livepeer-react.git",
    "directory": "packages/core"
  },
  "homepage": "https://docs.livepeer.org",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/es/index.d.mts",
        "default": "./dist/es/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./crypto": {
      "import": {
        "types": "./dist/es/crypto.d.mts",
        "default": "./dist/es/crypto.mjs"
      },
      "require": {
        "types": "./dist/cjs/crypto.d.ts",
        "default": "./dist/cjs/crypto.js"
      }
    },
    "./errors": {
      "import": {
        "types": "./dist/es/errors.d.mts",
        "default": "./dist/es/errors.mjs"
      },
      "require": {
        "types": "./dist/cjs/errors.d.ts",
        "default": "./dist/cjs/errors.js"
      }
    },
    "./external": {
      "import": {
        "types": "./dist/es/external.d.mts",
        "default": "./dist/es/external.mjs"
      },
      "require": {
        "types": "./dist/cjs/external.d.ts",
        "default": "./dist/cjs/external.js"
      }
    },
    "./media": {
      "import": {
        "types": "./dist/es/media.d.mts",
        "default": "./dist/es/media.mjs"
      },
      "require": {
        "types": "./dist/cjs/media.d.ts",
        "default": "./dist/cjs/media.js"
      }
    },
    "./storage": {
      "import": {
        "types": "./dist/es/storage.d.mts",
        "default": "./dist/es/storage.mjs"
      },
      "require": {
        "types": "./dist/cjs/storage.d.ts",
        "default": "./dist/cjs/storage.js"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/es/types.d.mts",
        "default": "./dist/es/types.mjs"
      },
      "require": {
        "types": "./dist/cjs/types.d.ts",
        "default": "./dist/cjs/types.js"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/es/utils.d.mts",
        "default": "./dist/es/utils.mjs"
      },
      "require": {
        "types": "./dist/cjs/utils.d.ts",
        "default": "./dist/cjs/utils.js"
      }
    },
    "./version": {
      "import": {
        "types": "./dist/es/version.d.mts",
        "default": "./dist/es/version.mjs"
      },
      "require": {
        "types": "./dist/cjs/version.d.ts",
        "default": "./dist/cjs/version.js"
      }
    }
  },
  "scripts": {
    "build": "bunchee",
    "clean": "rimraf .turbo node_modules dist",
    "dev": "bunchee --watch"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "multiformats": "^13.0.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "jose": "^5.2.0"
  },
  "keywords": [
    "livepeer",
    "video",
    "streaming",
    "livestream"
  ]
}
