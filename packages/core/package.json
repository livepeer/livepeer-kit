{
  "name": "livepeer",
  "description": "Livepeer's vanilla JS library.",
  "license": "MIT",
  "version": "1.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/livepeer/livepeer.js.git",
    "directory": "packages/core"
  },
  "homepage": "https://livepeerjs.org",
  "main": "dist/livepeer.cjs.js",
  "module": "dist/livepeer.esm.js",
  "types": "dist/livepeer.cjs.d.ts",
  "umd:main": "dist/livepeer.umd.min.js",
  "sideEffects": false,
  "files": [
    "/actions",
    "/constants",
    "/contracts",
    "/dist",
    "/media",
    "/providers",
    "/types",
    "/utils"
  ],
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "*/index.ts",
      "providers/*/index.ts"
    ],
    "umdName": "livepeer"
  },
  "peerDependencies": {
    "ethers": ">=5.6.9",
    "immer": ">=9.0",
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "ethers": {
      "optional": true
    },
    "immer": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "@stitches/core": "^1.2.8",
    "core-js": "^3.25.5",
    "cross-fetch": "^3.1.5",
    "hls.js": "^1.2.4",
    "tus-js-client": "^3.0.1",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "ethers": "^5.7.1"
  },
  "keywords": [
    "eth",
    "ethereum",
    "livepeer",
    "streaming",
    "video",
    "web3"
  ]
}
