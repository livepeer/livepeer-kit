{
  "name": "@livepeer/react-native",
  "description": "React Native components & hooks for mobile apps built with Livepeer.",
  "license": "MIT",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/livepeer/livepeer.js.git",
    "directory": "packages/react-native"
  },
  "homepage": "https://livepeerjs.org",
  "main": "dist/livepeer-react-native.cjs.js",
  "module": "dist/livepeer-react-native.esm.js",
  "types": "dist/livepeer-react-native.cjs.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "module": "./dist/livepeer-react-native.esm.js",
      "default": "./dist/livepeer-react-native.cjs.js"
    },
    "./components": {
      "module": "./components/dist/livepeer-react-native-components.esm.js",
      "default": "./components/dist/livepeer-react-native-components.cjs.js"
    },
    "./contracts": {
      "module": "./contracts/dist/livepeer-react-native-contracts.esm.js",
      "default": "./contracts/dist/livepeer-react-native-contracts.cjs.js"
    },
    "./hooks": {
      "module": "./hooks/dist/livepeer-react-native-hooks.esm.js",
      "default": "./hooks/dist/livepeer-react-native-hooks.cjs.js"
    }
  },
  "sideEffects": false,
  "files": [
    "client",
    "/components",
    "/context",
    "contracts",
    "hooks",
    "/dist"
  ],
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "client.ts",
      "components/index.ts",
      "context/index.ts",
      "contracts.ts",
      "hooks.ts"
    ]
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-native": ">=0.68.2"
  },
  "dependencies": {
    "@livepeer/core-react": "^1.0.0",
    "livepeer": "^1.4.0",
    "react-native-video": "6.0.0-alpha.3",
    "zustand": "^4.1.3"
  },
  "devDependencies": {
    "@tanstack/react-query": "4.14.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^11.4.0",
    "@types/react": "^18.0.23",
    "@types/react-native": "^0.70.6",
    "@types/react-native-video": "^5.0.14",
    "react": "^18.2.0",
    "react-native": "^0.70.5"
  },
  "keywords": [
    "dapps",
    "eth",
    "ethereum",
    "hooks",
    "livepeer",
    "react",
    "react-native",
    "mobile",
    "streaming",
    "video",
    "web3"
  ]
}
