{
  "name": "@livepeer/react-native",
  "description": "React Native components & hooks for mobile apps built with Livepeer.",
  "license": "MIT",
  "version": "1.0.0-next.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/livepeer/livepeer.js.git",
    "directory": "packages/react-native"
  },
  "homepage": "https://livepeerjs.org",
  "main": "dist/livepeer-react-native.cjs.js",
  "module": "dist/livepeer-react-native.esm.js",
  "types": "dist/livepeer-react-native.cjs.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "module": "./dist/livepeer-react-native.esm.js",
      "default": "./dist/livepeer-react-native.cjs.js"
    },
    "./components": {
      "module": "./components/dist/livepeer-react-native-components.esm.js",
      "default": "./components/dist/livepeer-react-native-components.cjs.js"
    },
    "./contracts": {
      "module": "./contracts/dist/livepeer-react-native-contracts.esm.js",
      "default": "./contracts/dist/livepeer-react-native-contracts.cjs.js"
    },
    "./hooks": {
      "module": "./hooks/dist/livepeer-react-native-hooks.esm.js",
      "default": "./hooks/dist/livepeer-react-native-hooks.cjs.js"
    }
  },
  "sideEffects": false,
  "files": [
    "client",
    "/components",
    "/context",
    "contracts",
    "hooks",
    "/dist"
  ],
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "client.ts",
      "components/index.ts",
      "context/index.ts",
      "contracts.ts",
      "hooks.ts"
    ]
  },
  "peerDependencies": {
    "expo": "*",
    "expo-av": ">=12.0.0",
    "react": ">=17.0.0",
    "react-native": ">=0.68.2",
    "react-native-svg": ">=13.0.0"
  },
  "peerDependenciesMeta": {},
  "dependencies": {
    "@livepeer/core-react": "^1.0.0-next.4",
    "react-native-url-polyfill": "^1.3.0",
    "stitches-native": "^0.3.2",
    "zustand": "^4.1.3"
  },
  "devDependencies": {
    "@tanstack/react-query": "4.18.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^11.5.0",
    "@types/react": "^18.0.23",
    "@types/react-native": "^0.70.7",
    "expo": "^47.0.6",
    "expo-av": "13.0.1",
    "react": "^18.2.0",
    "react-native": "0.70.5",
    "react-native-svg": "12.4.4"
  },
  "keywords": [
    "dapps",
    "eth",
    "ethereum",
    "hooks",
    "livepeer",
    "react",
    "react-native",
    "mobile",
    "streaming",
    "video",
    "web3"
  ]
}
