{
  "name": "@livepeer/react",
  "description": "React components & hooks for apps built with Livepeer.",
  "license": "MIT",
  "version": "1.0.6",
  "repository": {
    "type": "git",
    "url": "https://github.com/livepeer/livepeer.js.git",
    "directory": "packages/react"
  },
  "homepage": "https://livepeerjs.org",
  "main": "dist/livepeer-react.cjs.js",
  "module": "dist/livepeer-react.esm.js",
  "types": "dist/livepeer-react.cjs.d.ts",
  "umd:main": "dist/livepeer-react.umd.min.js",
  "sideEffects": false,
  "files": [
    "/components",
    "/contracts",
    "/dist",
    "/hooks",
    "/utils"
