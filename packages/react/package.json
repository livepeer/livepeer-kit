{
  "name": "@livepeer/react",
  "description": "React components & hooks for apps built with Livepeer.",
  "license": "MIT",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/livepeer/livepeer.js.git",
    "directory": "packages/react"
  },
  "homepage": "https://livepeerjs.org",
  "main": "dist/livepeer-react.cjs.js",
  "module": "dist/livepeer-react.esm.js",
  "types": "dist/livepeer-react.cjs.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "module": "./dist/livepeer-react.esm.js",
      "default": "./dist/livepeer-react.cjs.js"
    },
    "./contracts": {
      "module": "./contracts/dist/livepeer-react-contracts.esm.js",
      "default": "./contracts/dist/livepeer-react-contracts.cjs.js"
    }
  },
  "sideEffects": false,
  "files": [
    "/contracts",
    "/dist"
  ],
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "contracts/index.ts"
    ]
  },
  "peerDependencies": {
    "ethers": ">=5.5.1",
    "react": ">=17.0.0",
    "wagmi": ">=0.6.2"
  },
  "dependencies": {
    "@stitches/react": "^1.2.8",
    "@tanstack/query-sync-storage-persister": "4.2.3",
    "@tanstack/react-query": "4.2.3",
    "@tanstack/react-query-persist-client": "4.2.1",
    "core-js": "^3.25.2",
    "livepeer": "^1.0.1",
    "use-sync-external-store": "^1.2.0",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/use-sync-external-store": "^0.0.3",
    "ethers": "^5.7.0",
    "livepeer": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wagmi": "^0.6.4"
  },
  "keywords": [
    "dapps",
    "eth",
    "ethereum",
    "hooks",
    "livepeer",
    "react",
    "streaming",
    "video",
    "web3"
  ]
}
