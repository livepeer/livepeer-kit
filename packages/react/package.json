{
  "name": "@livepeer/react",
  "description": "React primitives for video apps.",
  "license": "MIT",
  "version": "4.0.0-next.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/livepeer/livepeer-react.git",
    "directory": "packages/react"
  },
  "homepage": "https://docs.livepeer.org",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/es/index.d.mts",
        "default": "./dist/es/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./assets": {
      "import": {
        "types": "./dist/es/assets.d.mts",
        "default": "./dist/es/assets.mjs"
      },
      "require": {
        "types": "./dist/cjs/assets.d.ts",
        "default": "./dist/cjs/assets.js"
      }
    },
    "./broadcast": {
      "import": {
        "types": "./dist/es/broadcast.d.mts",
        "default": "./dist/es/broadcast.mjs"
      },
      "require": {
        "types": "./dist/cjs/broadcast.d.ts",
        "default": "./dist/cjs/broadcast.js"
      }
    },
    "./crypto": {
      "import": {
        "types": "./dist/es/crypto.d.mts",
        "default": "./dist/es/crypto.mjs"
      },
      "require": {
        "types": "./dist/cjs/crypto.d.ts",
        "default": "./dist/cjs/crypto.js"
      }
    },
    "./external": {
      "import": {
        "types": "./dist/es/external.d.mts",
        "default": "./dist/es/external.mjs"
      },
      "require": {
        "types": "./dist/cjs/external.d.ts",
        "default": "./dist/cjs/external.js"
      }
    },
    "./player": {
      "import": {
        "types": "./dist/es/player.d.mts",
        "default": "./dist/es/player.mjs"
      },
      "require": {
        "types": "./dist/cjs/player.d.ts",
        "default": "./dist/cjs/player.js"
      }
    }
  },
  "scripts": {
    "build": "bunchee",
    "clean": "rimraf .turbo node_modules dist",
    "dev": "bunchee --watch",
    "lint": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@livepeer/core": "workspace:*",
    "@livepeer/core-react": "workspace:*",
    "@livepeer/core-web": "workspace:*",
    "@radix-ui/primitive": "^1.0.1",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-compose-refs": "^1.0.1",
    "@radix-ui/react-context": "^1.0.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-portal": "^1.0.4",
    "@radix-ui/react-presence": "^1.0.1",
    "@radix-ui/react-primitive": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-use-controllable-state": "^1.0.1",
    "@radix-ui/react-use-layout-effect": "^1.0.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.2.51",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "keywords": [
    "livepeer",
    "react",
    "video",
    "streaming",
    "livestream"
  ]
}
