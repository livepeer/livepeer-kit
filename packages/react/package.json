      "index.ts",
      "*/index.ts"
    ],
    "umdName": "@livepeer/react"
  },
  "peerDependencies": {
    "ethers": ">=5.5.1",
    "immer": ">=9.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "react-native": "*",
    "wagmi": ">=0.6.2"
  },
  "peerDependenciesMeta": {
    "ethers": {
      "optional": true
    },
    "immer": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "@stitches/react": "^1.2.8",
    "@tanstack/query-sync-storage-persister": "4.10.1",
    "@tanstack/react-query": "4.10.1",
    "@tanstack/react-query-persist-client": "4.10.1",
    "core-js": "^3.25.5",
    "livepeer": "workspace:^1.0.4",
    "use-sync-external-store": "^1.2.0",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/use-sync-external-store": "^0.0.3",
    "ethers": "^5.7.1",
    "livepeer": "^1.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wagmi": "^0.6.8"
  },
  "keywords": [
    "dapps",
    "eth",
    "ethereum",
    "hooks",
    "livepeer",
    "react",
    "streaming",
    "video",
    "web3"
  ]
}
